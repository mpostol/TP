<Project Sdk="Microsoft.NET.Sdk">

  <PropertyGroup>
    <OutputType>WinExe</OutputType>
    <TargetFramework>net8.0-windows7.0</TargetFramework>
    <Nullable>enable</Nullable>
    <UseWPF>true</UseWPF>
    <Version>6.0.0</Version>
    <Authors>MPostol</Authors>
    <Company>MPostol</Company>
    <PackageProjectUrl>https://www.udemy.com/user/mariusz-postol/</PackageProjectUrl>
    <RepositoryUrl>https://github.com/mpostol/TP</RepositoryUrl>
    <AssemblyName>TP.ExDM.GraphicalData.View</AssemblyName>
    <RootNamespace>TP.ExDM.GraphicalData.View</RootNamespace>
